#include <iostream>
using namespace std;
void printDigits(int n){


  
  //base case 
  if(n==0){

    
    return;
  }
  int newValueOfn=n/10;
    //baaki recursion solve krlega 
  printDigits( newValueOfn);
  //1 case ma solve karunga
  int digit=n%10;
  cout<<digit<<" ";

}
int main() {
int n =6470;
if(n==0){
  cout<<0<<endl;
}
  printDigits(n);
  return 0;
}
