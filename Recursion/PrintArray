#include <iostream>
using namespace std;



//TAIL RECURSION(BASE CONDITION IS AFTER PROCESSING AT THE END)
  void  print(int arr[],int n,int i){
   
    //base case 
    if(i>=n){
      return;
    }
    

    //baaki recursion dekhlega 
    print(arr,n,i+1);
     //1 case solve krdia
    cout<<arr[i]<<" ";
  }
int main() {



  int arr[5]={23,56,17,87,98};
  int n=5;
  int i=0;
  print(arr,n,i);


